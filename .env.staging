DOMAIN_NAME=<REPLACE_WITH_YOUR_SERVICE_DNS_NAME>
TLD_ZONE_ID=<REPLACE_WITH_AWS_ROUTE_53_HOSTED_ZONE_ID_OF_TOP_LEVEL_DOMAIN>
AWS_ACCESS_KEY_ID=<REPLACE_WITH_YOUR_AWS_ACCESS_KEY_ID>
AWS_SECRET_ACCESS_KEY=<REPLACE_WITH_YOUR_AWS_SECRET_ACCESS_KEY>
PUBLIC_EC2_KEY=<REPLACE_WITH_YOUR_PUBLIC_SSH_KEY>
REGION=ap-northeast-3
## Optional: environment name used for Terraform (maps to TF_VAR_environment)
## Common values: dev | staging | prod
ENVIRONMENT=staging
## Optional: prefix for local Docker image tag used during build
# Example: set to your Docker Hub username, e.g. "yourname"
# Leave empty to build without a namespace prefie
LOCAL_IMAGE_NAMESPACE=
SERVICE_NAME=scenario-aws-ecs-ec2
